{
  "name": "fondcas",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "sync:download": "npx tsx scripts/download-cas-data.ts",
    "sync:download-historical": "npx tsx scripts/download-historical-data.ts",
    "sync:parse": "npx tsx scripts/parse-providers.ts && npx tsx scripts/parse-funds.ts",
    "sync:parse-historical": "npx tsx scripts/parse-historical-funds.ts",
    "sync:geocode": "npx tsx scripts/geocode-addresses.ts",
    "sync:upload": "npx tsx scripts/sync-to-db.ts",
    "sync:all": "npm run sync:download && npm run sync:parse && npm run sync:geocode && npm run sync:upload",
    "ml:train": "npx tsx scripts/train-prediction-model.ts",
    "ml:full-pipeline": "npm run sync:download-historical && npm run sync:parse-historical && npm run ml:train",
    "v2:parse": "npx tsx scripts/parse-cnas-data-v2.ts",
    "v2:enrich": "npx tsx scripts/ai-enrich-organizations.ts",
    "v2:enrich-continue": "npx tsx scripts/ai-enrich-organizations.ts --continue",
    "v2:sync": "npx tsx scripts/sync-v2-to-db.ts",
    "v2:sync-geocode": "npx tsx scripts/sync-v2-to-db.ts --geocode",
    "v2:full": "npm run v2:parse && npm run v2:enrich && npm run v2:sync-geocode"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.72.1",
